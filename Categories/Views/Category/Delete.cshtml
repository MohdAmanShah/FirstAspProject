@model Category
@{
	ViewData["Title"] = "Delete Category";
}
<style>
	.butn {
		width: 150px;
	}
</style>
<form method="post">
	<div class="border p-3 mt-4">
		<div class="row pb-2">
			<h2 class="text-primary">Edit Category</h2>
		</div>
		<div class="mb-3">
			<label asp-for="Name"></label>
			<input asp-for="Name" class="form-control" disabled="" />
		</div>
		<div class="mb-3">
			<label asp-for="DisplayOrder"></label>
			<input asp-for="DisplayOrder" class="form-control" disabled="" />
		</div>
		<button type="submit" class="btn btn-primary butn">Delete</button>
		<a asp-controller="Category" asp-action="Index" class="btn btn-dark butn">
			Back to List
		</a>
	</div>
</form>

@section Scripts{
	@{
		<partial name="_ValidationScriptsPartial" />
	}
}